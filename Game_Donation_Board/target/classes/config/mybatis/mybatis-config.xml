<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
		<typeAlias type="dto.ProjectMainListDTO" alias="mainlist"/>
	<typeAlias type="dto.NoticeDTO" alias="notice"/>
		<typeAlias type="dto.AdminInfoDTO" alias="admininfo"/>
		<typeAlias type="dto.ProjectDTO" alias="project"/> 
		<typeAlias type="dto.UserDTO" alias="user"/> 
		<typeAlias type="dto.CategoryNumDTO" alias="categoryNum"/> 
		<typeAlias type="dto.AdminNoticeDTO" alias="adminNotice"/> 
		<typeAlias type="dto.ReviewDTO" alias="review"/> 
		  
	</typeAliases>
		
	<mappers>
		<mapper resource="config/mybatis/mapper/user.xml" />
		<mapper resource="config/mybatis/mapper/project.xml" />
		<mapper resource="config/mybatis/mapper/detail.xml" />
		<mapper resource="config/mybatis/mapper/notice.xml" />
		<mapper resource="config/mybatis/mapper/profile.xml"/>
		</mappers>
</configuration>